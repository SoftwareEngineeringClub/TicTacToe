<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class 
	    abstract="false" 
	    name="tictactoe.domain.gamedomain.Game" 
	    table="Game">
  		<id 
  	    	column="GameId" 
  	    	name="GameId" 
  	    	type="java.lang.Long">
   			<generator class="native"/>
  		</id>
  		<version 
  		    column="Version" 
  		    generated="never" 
  		    name="Version" 
  		    type="java.lang.Integer"/>
   		<property 
   		    generated="never" 
   		    lazy="false" 
   		    name="PlayerIdX" 
   		    column="PlayerIdX"
   		    type="java.lang.Long"/>
   		<property 
   		    generated="never" 
   		    lazy="false" 
   		    name="PlayerIdO" 
   		    column="PlayerIdO"
   		    type="java.lang.Long"/>
   		<property 
   		    generated="never" 
   		    lazy="false" 
   		    name="CurrentPlayerId" 
   		    column="CurrentPlayerId"
   		    type="java.lang.Long"/>
        <property 
   		    generated="never" 
   		    lazy="false" 
   		    name="Start" 
   		    column="Start"
   		    not-null="false"
            type="strata1.entity.hibernaterepository.DateTimeType"/>
        <property 
   		    generated="never" 
   		    lazy="false" 
            name="Finish" 
            column="Finish"
            not-null="false"
            type="strata1.entity.hibernaterepository.DateTimeType"/>
		<property 
   		    generated="never" 
   		    lazy="false" 
   		    name="Dimension" 
   		    column="Dimension"
   		    type="java.lang.Integer"/>
   		<property 
   		    generated="never" 
   		    lazy="false" 
   		    name="State" 
   		    column="State"
   		    type="java.lang.String"/>
 	</class>
</hibernate-mapping>
